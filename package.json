    "vue2-filters": "0.3.0",
    "vuelidate": "0.7.4",
    "vuex": "3.0.1"
  },
  "devDependencies": {
    "@types/jest": "23.3.10",
    "@types/node": "10.12.18",
    "@types/vuelidate": "0.7.3",
    "@vue/cli-plugin-typescript": "3.2.2",
    "@vue/cli-service": "3.2.2",
    "@vue/test-utils": "1.0.0-beta.27",
    "autoprefixer": "7.2.6",
    "browser-sync": "2.26.3",
    "browser-sync-webpack-plugin": "2.2.2",
    "copy-webpack-plugin": "4.6.0",
